{
  "name": "lmda",
  "version": "0.2.0",
  "description": "Official La Murga de Austin site",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@erquhart/rollup-plugin-node-builtins": "^2.1.5",
    "@tbranyen/jsdom": "^13.0.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "eleventy-plugin-toc": "^1.1.5",
    "html-minifier": "^4.0.0",
    "image-size": "^1.0.0",
    "install": "^0.13.0",
    "json-to-scss": "^1.6.2",
    "make-dir-cli": "^3.0.0",
    "node-fetch": "^2.6.6",
    "npm": "^8.2.0",
    "rollup": "^2.60.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass": "^1.44.0"
  },
  "scripts": {
    "sass:tokens": "npx json-to-scss src/_data/tokens.json src/scss/_tokens.scss",
    "sass:process": "npm run sass:tokens && sass src/scss/global.scss src/_includes/assets/css/global.css --style=compressed",
    "cms:precompile": "make-dir dist/admin && nunjucks-precompile src/_includes > dist/admin/templates.js -i \"\\.(njk|css|svg)$\"",
    "cms:bundle": "rollup --config",
    "serve": "cross-env ELEVENTY_ENV=development npx eleventy --serve",
    "production": "npm run sass:process && npm run cms:precompile && npm run cms:bundle && npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/los-verdes/lmda.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/los-verdes/lmda/issues"
  },
  "homepage": "https://github.com/los-verdes/lmda#readme"
}
